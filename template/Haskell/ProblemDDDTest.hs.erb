module Main where

import Test.HUnit
import System
import Problem<%= number %>

<%= function %>Tests = TestList [
% assertions.each do |expected, computation|
  TestCase (<%= expected %> @=? <%= function %> <%= computation %>),
% end
  TestCase (1 @=? 1)
  ]

checkTests counts = case counts of
	Counts _ _ errors failures ->
		if (errors + failures > 0)
		then exitFailure
		else (exitWith ExitSuccess)

main = runTestTT (TestLabel "testing <%= function %>" <%= function %>Tests)  >>= checkTests
