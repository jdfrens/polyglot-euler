#!/usr/bin/env swipl -q -t go -s

go :- 
	consult(problem<%= number %>),
	run_tests.

:- begin_tests(problem<%= number %>).

% functions.each do |function|
% tests(function).each do |expected, computation|
test(<%= function %>) :- <%= function %>(<%= computation %>, <%= expected %>).
% end

% end

:- end_tests(problem<%= number %>).
